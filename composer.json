{
    "name": "exadra37/package-skeleton",
    "description": "package_description",
    "keywords": [
        "Exadra37",
        "Package Skeleton"
    ],
    "homepage": "https://github.com/exadra37/package-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Paulo Silva",
            "email": "exadra@gmail.com",
            "homepage": "http://exadra37.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpmd/phpmd" : "@stable",
        "phpspec/phpspec": "^2.4",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "2.*",
        "orchestra/testbench": "^3.2",
        "benconstable/phpspec-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Exadra37\\PackageSkeleton\\": "src",
            "PhpSpec\\Laravel\\": "vendor/exadra37/phpspec-laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exadra37\\Skeleton\\Test\\": "tests"
        },
        "psr-0": {
            "PhpSpec\\Laravel\\": "vendor/exadra37/phpspec-laravel/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
